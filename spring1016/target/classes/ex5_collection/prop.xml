<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">
	<!-- 외부 property file 불러들이기 -->
	<context:property-placeholder location="classpath:ex5_collection/*.properties"/>
	<bean id="prop" class="ex5_collection.Ex3_Properties">
		<property name="prop">
			<props>
				<prop key="web">https://ictedu.co.kr</prop>
				<prop key="dbid">${id}</prop>
				<prop key="dbpwd">${pwd}</prop>
				<prop key="dburl">${url}</prop>
			</props>
		</property>
	</bean>

</beans>
